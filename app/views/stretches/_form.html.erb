<%= form_for(@stretch) do |f| %>
  <%= f.error_messages %>
  <p>
    <%= f.label :recorded_on %>
    <%= f.text_field :recorded_on %>
  </p>
  <p>
    <%= f.label :duration %>
    <%= f.text_field :duration %>
  </p>
  <p>
    <%= f.label :starting_quarter %>
    <%= f.text_field :starting_quarter %>
  </p>
  <p>
    <%= f.label :note %>
    <%= f.text_field :note %>
  </p>
  <p>
    <%= f.label :activity_id %>
    <%= f.text_field :activity_id %>
  </p>

  <%= f.submit %>
<% end %>
